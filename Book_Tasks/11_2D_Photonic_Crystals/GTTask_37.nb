Cell["\<\
The visualization of  a single band can de defined as follows in a small \
module.\
\>", "Text",
 CellChangeTimes->{{3.716521114215784*^9, 3.716521162642321*^9}, {
   3.716521533029809*^9, 3.7165215596805353`*^9}, 
   3.716523020690127*^9},ExpressionUUID->"9eb5cbf8-57d0-493a-8860-\
808ace82fd12"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SingleBand", "[", " ", 
   RowBox[{"kvec_", ",", "gvec_", ",", "bandplot_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", 
    RowBox[{
     RowBox[{"kv", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Transpose", "[", 
         RowBox[{"kvec", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}], "//", "N"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ev", "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Norm", "[", 
          RowBox[{"#", "-", "gvec"}], "]"}], "&"}], "/@", 
        RowBox[{
         RowBox[{"Transpose", "[", 
          RowBox[{"kvec", "[", 
           RowBox[{"[", "1", "]"}], "]"}], "]"}], "[", 
         RowBox[{"[", "3", "]"}], "]"}]}], "//", "N"}]}], ";", 
     RowBox[{"p1", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"kv", ",", "ev"}], "}"}], "//", "Transpose"}], ",", 
        RowBox[{"Joined", "\[Rule]", "True"}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", "Red"}]}], "]"}]}], ";", 
     RowBox[{"Show", "[", 
      RowBox[{"bandplot", ",", "p1"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.716521167483795*^9, 3.7165212294893847`*^9}, {
   3.7165215636410427`*^9, 3.716521677324143*^9}, {3.716521708038652*^9, 
