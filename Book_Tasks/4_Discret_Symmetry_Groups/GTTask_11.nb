Cell[CellGroupData[{

Cell[TextData[{
 "Part I: Proof :The product of a ",
 Cell[BoxData[
  FormBox[
   RowBox[{"rotation", " ", 
    SubsuperscriptBox["C", "n", "m"], " ", "with", " ", "a", " ", "mirror", 
    " ", "operation", " ", 
    SubscriptBox["\[Sigma]", "v"], " ", "results", " ", "in", " ", "a", " ", 
    "mirror", " ", "operation"}], TraditionalForm]],ExpressionUUID->
  "93d24783-4d2a-46d5-8eab-3bf3d93149bb"],
 ". The mirror plane contains the rotation axis."
}], "Item",
 CellChangeTimes->{{3.71470035792348*^9, 3.714700411737934*^9}, {
  3.74547768564958*^9, 
  3.745477716377573*^9}},ExpressionUUID->"a385757a-2446-419f-8e20-\
be3a355e5817"],

Cell[TextData[{
 "Part II:  For even ",
 StyleBox["n",
  FontSlant->"Italic"],
 " two sets of mirror planes exist ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["\[Sigma]", "v"], ",", 
     RowBox[{
      SubscriptBox["\[Sigma]", "v"], "'"}]}]}], TraditionalForm]],
  ExpressionUUID->"cb7b9b48-f0f9-405e-946b-c9a3e634b4b3"],
 ",... and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Sigma]", "d"], ",", 
    RowBox[{
     SubscriptBox["\[Sigma]", "d"], "'"}], ","}], TraditionalForm]],
  ExpressionUUID->"4f38a8ef-29af-4d6d-8730-acce20d5ea27"],
 "...). The multiplication of the rotation with a member of one of the sets \
leads to another member of the same set."
}], "Item",
 CellChangeTimes->{{3.71470035792348*^9, 
  3.714700434112731*^9}},ExpressionUUID->"bb2323e3-0d14-4a07-91c1-\
1ebd661ec1df"],

