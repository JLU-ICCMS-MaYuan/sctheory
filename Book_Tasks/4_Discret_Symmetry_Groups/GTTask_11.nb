Cell[TextData[{
 "At first we construct a rotation matrix for a rotation ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["C", "n", "m"], TraditionalForm]],ExpressionUUID->
  "b7789458-b413-4589-8b3d-61ab8d1176ee"],
 " about the ",
 StyleBox["z",
  FontSlant->"Italic"],
 "-axis. We have to take into account that we work in the passive notation. \
We have to take the rotation angle or the direction of the rotation axis \
negative."
}], "Text",
 CellChangeTimes->{{3.714695908798546*^9, 3.714696063071704*^9}, {
  3.7147004987691383`*^9, 
  3.714700500478713*^9}},ExpressionUUID->"abdb3fc5-ff6e-476d-a138-\
d22fe1e21310"],

Cell[BoxData[
 RowBox[{
  RowBox[{"rotmat", "[", 
   RowBox[{"m_", ",", "n_"}], "]"}], "=", 
  RowBox[{"RotationMatrix", "[", 
   RowBox[{
    RowBox[{"m", " ", "2", 
     RowBox[{"\[Pi]", "/", "n"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"0", ",", "0", ",", 
      RowBox[{"-", "1"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.714664426678603*^9, 3.714664444076339*^9}, {
   3.714664488887323*^9, 3.714664552386333*^9}, {3.7146654753555517`*^9, 
