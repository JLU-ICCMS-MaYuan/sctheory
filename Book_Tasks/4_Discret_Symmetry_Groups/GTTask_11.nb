   3.7146958586042*^9, 3.7146958616576147`*^9}, {3.714695953565371*^9, 
   3.714695956728807*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"48cae423-a4ba-4166-a0d2-0d28d90c41b7"],

