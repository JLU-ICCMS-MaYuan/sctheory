Cell["\<\
The module to construct symmetry adopted linear combinations (SALC\
\[CloseCurlyQuote]s) from Figure 9.7 is slightly changed. Now it is possible \
to define an input combination to be symmetrized. The SALC\[CloseCurlyQuote]s \
are not normalized in this version. The normalization will be done at the \
end. \
\>", "Text",
 CellChangeTimes->{{3.716282782155982*^9, 3.716282867915142*^9}, {
  3.716282963980398*^9, 3.716282994435068*^9}, {3.745546430228335*^9, 
  3.7455464377854013`*^9}},ExpressionUUID->"ff2465d6-c45a-4c79-a4d8-\
0abbdddc7a7e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SALC", "[", 
   RowBox[{
   "grp_", ",", "prep_", ",", "signs_", ",", "chars_", ",", "\[Phi]_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "cls", ",", "class", ",", "el", ",", "pos", ",", "k", ",", "salc", ",", 
      "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"salc", "=", 
      RowBox[{"Table", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{"Length", "[", "\[Phi]", "]"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cls", "   ", "=", 
      RowBox[{"GTClasses", "[", "grp", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", " ", "[", "\[IndentingNewLine]", "     ", 
      RowBox[{
       RowBox[{
        RowBox[{"class", "=", 
         RowBox[{"cls", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "     ", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", "         ", 
         RowBox[{
          RowBox[{
           RowBox[{"el", "    ", "=", 
            RowBox[{"class", "[", 
             RowBox[{"[", "k", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           "         ", 
           RowBox[{"pos", "  ", "=", 
            RowBox[{
             RowBox[{"Flatten", "[", 
              RowBox[{"Position", "[", 
               RowBox[{"grp", ",", "el"}], "]"}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
           "         ", 
           RowBox[{"salc", "=", 
            RowBox[{"salc", "+", 
             RowBox[{
              RowBox[{"signs", "[", 
               RowBox[{"[", "i", "]"}], "]"}], " ", 
              RowBox[{"chars", "[", 
               RowBox[{"[", "i", "]"}], "]"}], " ", 
              RowBox[{
               RowBox[{"prep", "[", 
                RowBox[{"[", "pos", "]"}], "]"}], ".", "\[Phi]"}]}]}]}]}], 
          "\[IndentingNewLine]", " ", ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", 
            RowBox[{"Length", "[", "class", "]"}]}], "}"}]}], "]"}]}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "cls", "]"}]}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "   ", 
     RowBox[{"salc", "=", "salc"}], ";", 
     RowBox[{"Return", "[", "salc", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
