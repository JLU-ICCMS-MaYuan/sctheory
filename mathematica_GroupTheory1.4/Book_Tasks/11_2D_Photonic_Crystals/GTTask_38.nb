Cell[CellGroupData[{

Cell["Calculation of the DOS using MPB", "Subsection",
 CellChangeTimes->{{3.716609656295424*^9, 
  3.716609664675829*^9}},ExpressionUUID->"818afdfd-62c5-428e-84f4-\
bdd471bf35eb"],

Cell[TextData[{
 "The calculation of the DOS for photonic band structures in ",
 StyleBox["GTPack ",
  FontWeight->"Bold"],
 "follows the same lines as DOS calculations in the electronic case. We have \
to calculate the band structure at enough ",
 StyleBox["k-",
  FontWeight->"Bold"],
 "points in the irreducible part of the BZ, followed by the DOS calculation. ",
 StyleBox[" GTDensityOfStates ",
  FontWeight->"Bold"],
 "allows to do all in one step.",
 StyleBox[" ",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.71660967926724*^9, 3.716609713017853*^9}, {
  3.7166099291583843`*^9, 3.7166099442665043`*^9}, {3.716610041155167*^9, 
  3.7166101794637938`*^9}, {3.7455854276563673`*^9, 
  3.745585454885253*^9}},ExpressionUUID->"9e361318-ae39-49e8-a1f7-\
6157e2d7f436"],

Cell["We do the calculation for TM polarisation", "Text",
 CellChangeTimes->{{3.716610210402499*^9, 
  3.716610218980545*^9}},ExpressionUUID->"52b14cda-c6dd-4abd-a2e7-\
fa02fea901ec"],

Cell[BoxData[
 RowBox[{
  RowBox[{"kpts", "=", 
   RowBox[{"GTBZPointMesh", "[", 
    RowBox[{"30", ",", "1", ",", "\"\<square\>\""}], "]"}]}], ";"}]], "Input",
 CellLabel->"In[21]:=",ExpressionUUID->"8ca0fc2f-75d9-4b32-95b4-1c1db2883e01"],

Cell[BoxData[
 RowBox[{
  RowBox[{"bands", "=", 
   RowBox[{"GTBands", "[", 
    RowBox[{"hmtm", ",", "kpts", ",", "10", ",", 
     RowBox[{"GOPhotonic", "\[Rule]", "True"}], ",", 
     RowBox[{"GOPlotBands", "\[Rule]", "False"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.716611143148198*^9, 3.7166111445417957`*^9}},
 CellLabel->"In[22]:=",ExpressionUUID->"e47ca4f3-c504-4e7d-86fe-a3882213c3e8"],

