 RowBox[{
  RowBox[{"kps1", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"1", "/", "2"}], ",", 
         RowBox[{"1", "/", "2"}], ",", "0"}], "}"}], ",", "\"\<M\>\""}], 
      "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "0"}], "}"}], ",", 
       "\"\<\[CapitalGamma]\>\""}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         FractionBox["1", "2"], ",", "0", ",", "0"}], "}"}], ",", 
       "\"\<X\>\""}], "}"}]}], "}"}]}], ";"}]], "Input",
 CellLabel->"In[26]:=",ExpressionUUID->"3a2f0031-a41c-45b6-b30d-e4baeb84c115"],

