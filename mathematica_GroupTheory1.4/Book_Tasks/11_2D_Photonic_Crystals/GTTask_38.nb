   RowBox[{"GTBands", "[", 
    RowBox[{"hmtm", ",", "kpts", ",", "10", ",", 
     RowBox[{"GOPhotonic", "\[Rule]", "True"}], ",", 
     RowBox[{"GOPlotBands", "\[Rule]", "False"}]}], "]"}]}], ";"}]], "Input",
