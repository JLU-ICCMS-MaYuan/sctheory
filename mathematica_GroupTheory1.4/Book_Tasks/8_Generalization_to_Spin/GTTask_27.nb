Cell[TextData[{
 "The following splitting that belong to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "g"], TraditionalForm]],ExpressionUUID->
  "55455916-9554-494b-a0c7-cb4bc5c58a88"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["T", 
    RowBox[{"2", "g"}]], TraditionalForm]],ExpressionUUID->
  "614cd6b7-8cac-4963-a95d-9e276b11525b"],
 " in a cubic field with respect to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["D", 
    RowBox[{"4", "h"}]], TraditionalForm]],ExpressionUUID->
  "c9b9bc6b-c2ee-4de9-9d78-35e0fca15e4e"],
 " was obtained:\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "g"], TraditionalForm]],ExpressionUUID->
  "5469fc24-3524-452b-ad96-6a4b9f13513a"],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["A", 
      RowBox[{"1", "g"}]], "\[CirclePlus]", " ", 
     SubscriptBox["B", 
      RowBox[{"1", "g"}]]}], " ", ",", " ", 
    SubscriptBox["T", 
     RowBox[{"2", "g"}]]}], TraditionalForm]],ExpressionUUID->
  "d2830993-23a7-48fc-a678-e3c7308bebdb"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    SubscriptBox["B", 
     RowBox[{"2", "g"}]]}], TraditionalForm]],ExpressionUUID->
  "af5499b3-1290-46a5-bb1a-2a1f2e6f5594"],
 "\[CirclePlus] ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "g"], TraditionalForm]],ExpressionUUID->
  "6a53be17-60d6-4f73-9783-1baaa4e63a7f"],
