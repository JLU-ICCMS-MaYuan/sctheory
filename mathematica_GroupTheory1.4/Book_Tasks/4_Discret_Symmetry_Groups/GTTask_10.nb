Cell[TextData[{
 "We have the threefold main rotational axis and 3 twofold rotational axes \
perpendicular to the threefold axis, lying in the ",
 StyleBox["x-y",
  FontSlant->"Italic"],
 "-plane. The group contains 4 reflection planes represented by the product \
of the inversion and a twofold rotation. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["IC", 
     RowBox[{"2", "z"}]], " ", "is", " "}], TraditionalForm]],ExpressionUUID->
  "4825208d-d934-4dcf-9370-388192fa0cc3"],
 "a reflection at the ",
 StyleBox["x-y-",
  FontSlant->"Italic"],
 "plane.\nThis can be seen easily from the transformation matrix"
}], "Text",
 CellChangeTimes->{{3.651068999640848*^9, 3.651069013125792*^9}, {
   3.651069044502479*^9, 3.651069099448176*^9}, {3.651069187931222*^9, 
   3.651069354206004*^9}, 3.698590420827723*^9, {3.714638103576333*^9, 
   3.7146381344846277`*^9}},ExpressionUUID->"c88f0695-fdd2-4e33-b013-\
bdccbc3d2883"],

Cell[BoxData[
 RowBox[{
  RowBox[{"IC2z", "//", "GTGetMatrix"}], "//", "MatrixForm"}]], "Input",
 CellChangeTimes->{{3.651069359826251*^9, 3.6510693767531357`*^9}},
 CellLabel->"In[76]:=",ExpressionUUID->"2d927830-446d-4567-a45f-9ea0e5e5e200"],

Cell["Now we will calculate all the products of the reflections.", "Text",
 CellChangeTimes->{{3.651069633256719*^9, 
  3.6510696516123133`*^9}},ExpressionUUID->"c68b5b76-80cf-482d-ad85-\
880a7c635329"],

Cell[BoxData[
 RowBox[{
  RowBox[{"reflections", "=", 
   RowBox[{"{", 
    RowBox[{"IC2D", ",", "IC2C", ",", "IC2y", ",", "IC2z"}], "}"}]}], ";", 
  RowBox[{"mat", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", "4", "}"}], ",", 
     RowBox[{"{", "4", "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.651069659144704*^9, 3.6510697146480503`*^9}},
 CellLabel->"In[77]:=",ExpressionUUID->"5f4619f8-7cc0-4b29-be64-349fd4f9b736"],

Cell[BoxData[
 RowBox[{"Do", "[", "\[IndentingNewLine]", "   ", 
  RowBox[{
   RowBox[{"Do", "[", "\[IndentingNewLine]", "       ", 
    RowBox[{
     RowBox[{
      RowBox[{"mat", "[", 
       RowBox[{"[", 
        RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
      RowBox[{
       RowBox[{"reflections", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "\[SmallCircle]", 
       RowBox[{"reflections", "[", 
        RowBox[{"[", "j", "]"}], "]"}]}]}], "\[IndentingNewLine]", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "4"}], "}"}]}], "]"}], 
   "\[IndentingNewLine]", ",", 
   RowBox[{"{", 
    RowBox[{"j", ",", "1", ",", "4"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.651069718459613*^9, 3.651069817833152*^9}},
 CellLabel->"In[78]:=",ExpressionUUID->"26172ec4-5c33-4993-b045-4a45e6f6ba68"],

Cell[BoxData[
 RowBox[{"mat", "//", "MatrixForm"}]], "Input",
